{
  "name": "json5",
  "version": "2.2.1",
  "description": "JSON for Humans",
  "main": "lib/index.js",
  "browser": "dist/json5.umd.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "dist",
    "package.json5"
  ],
  "engines": {
    "node": "^14 || ^16 || >=18",
    "npm": ">=7"
  },
  "scripts": {
    "build": "run-s -s clean rollup",
    "changelog:check": "node scripts/changelog/check.js",
    "changelog:update": "node scripts/changelog/update.js",
    "clean": "rimraf dist/*",
    "coverage": "run-s -c -s tap tap:coverage",
    "dependencies": "npm run -s package:update",
    "eslint": "eslint --fix .",
    "lint": "run-s -s eslint prettier",
    "package:update": "node scripts/package/update.js",
    "prepack": "npm run -s build",
    "prepare": "husky install",
    "prettier": "prettier --loglevel warn -w .",
    "preversion": "run-s -s build test",
    "rollup": "rollup -c",
    "tap": "tap -R spec test",
    "tap:coverage": "tap --coverage-report html",
    "test": "run-s -s lint tap",
    "unicode:update": "node scripts/unicode/update.js",
    "version": "run-s -s \"changelog:update -- --add\" \"package:update -- --add\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/json5/json5.git"
  },
  "keywords": [
    "json",
    "json5",
    "es5",
    "es2015",
    "ecmascript"
  ],
  "author": "Aseem Kishore <aseem.kishore@gmail.com>",
  "contributors": [
    "Max Nanasy <max.nanasy@gmail.com>",
    "Andrew Eisenberg <andrew@eisenberg.as>",
    "Jordan Tucker <jordanbtucker@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/json5/json5/issues"
  },
  "homepage": "http://json5.org/",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/parse": "^17.0.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@tsconfig/node14": "^1.0.3",
    "@unicode/unicode-14.0.0": "^1.3.1",
    "chalk": "^5.0.1",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "core-js": "^3.25.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "regenerate": "^1.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "simple-git": "^3.14.1",
    "tap": "^16.3.0"
  }
}
